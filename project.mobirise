          "_name": "witsec-html-block",
          "_sourceTheme": "startm5",
          "_customHTML": "<section class=\"mbr-section mbr-section--no-padding\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-fullscreen': fullScreen, 'mbr-parallax-background': bg.parallax}\">\n\n<mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\" checked>\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n\n    <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\" parallax>\n    <input type=\"image\" title=\"Background Image\" value=\"../_images/background.jpg\" parallax>\n    <input type=\"color\" title=\"Background Color\" value=\"#efefef\" selected>\n    <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n    </fieldset>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n    <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    \n    <!-- Text Settings -->\n    <header>Text Settings</header>\n    <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n    <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n    <input type=\"color\" title=\"Card Color\" name=\"cardColor\" value=\"#fafafa\" selected>\n    <input type=\"color\" title=\"Text Color\" name=\"textColor\" value=\"#333333\" selected>\n</mbr-parameters>\n\n<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n</div>\n\n<!-- Place your own code below -->\n\n<div class=\"panel-grid\">\n    <!-- Column 1 -->\n    <div class=\"panel-grid-cell\">\n        <div class=\"so-panel widget widget_text\">\n            <div class=\"panel-widget-style\">\n                <h3 class=\"widget-title\">About</h3>\n                <div class=\"textwidget\">\n                    <h6 class=\"card-title\" mbr-if=\"showTitle\" style=\"color: {{textColor}};\">\n                        <b>Bonjour !</b><br><b>Bienvenue sur mon Portfolio !</b>\n                    </h6>\n                    <p class=\"mbr-text\" mbr-if=\"showText\" style=\"color: {{textColor}};\">\n                        Je suis une artiste et graphiste passionnée, spécialisée dans la création de logos, illustrations, et design pour le web. <br><br>\n                        Mon approche est à la fois créative et réfléchie, visant à transmettre l’essence de chaque projet à travers un design personnalisé. <br><br> Bonne visite !\n                    </p>\n                </div>\n            </div>\n        </div>\n        <div class=\"so-panel widget widget_sydney_contact_info\">\n            <div class=\"contact-email\">\n                <span><i class=\"fa fa-envelope\"></i></span>\n                <a href=\"mailto:aileen@kiwichameleon.com\">aileen@kiwichameleon.com</a>\n            </div>\n        </div>\n    </div>\n\n    <!-- Column 2 -->\n    <div class=\"panel-grid-cell\">\n        <div class=\"so-panel widget widget_sydney_image_widget\">\n            <div class=\"panel-widget-style\">\n                <div class=\"sydney-image\">\n                    <img src=\"https://www.kiwichameleon.com/wp-content/uploads/2019/07/KiwiArt_websiteSmall-2.png\" alt=\"Kiwi Chameleon Art\">\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <!-- Column 3 -->\n    <div class=\"panel-grid-cell\">\n        <div class=\"so-panel widget widget_sydney_skills\">\n            <div class=\"panel-widget-style\">\n                <h3 class=\"widget-title\">My Tools</h3>\n                <div class=\"roll-progress\">\n                    <div class=\"name\">Markers, mixed media</div>\n                    <div class=\"progress-bar\">\n                        <div class=\"progress-animate\" style=\"width: 91%;\"></div>\n                    </div>\n                </div>\n                <div class=\"roll-progress\">\n                    <div class=\"name\">Ink</div>\n                    <div class=\"progress-bar\">\n                        <div class=\"progress-animate\" style=\"width: 51%;\"></div>\n                    </div>\n                </div>\n                <div class=\"roll-progress\">\n                    <div class=\"name\">Painter, Photoshop</div>\n                    <div class=\"progress-bar\">\n                        <div class=\"progress-animate\" style=\"width: 85%;\"></div>\n                    </div>\n                </div>\n                <div class=\"roll-progress\">\n                    <div class=\"name\">Sketches II, Procreate</div>\n                    <div class=\"progress-bar\">\n                        <div class=\"progress-animate\" style=\"width: 71%;\"></div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n\n<!-- Place your own code above -->\n\n</section>",
